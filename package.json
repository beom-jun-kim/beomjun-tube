  "dependencies": {
    "@ffmpeg/core": "^0.11.0",
    "@ffmpeg/ffmpeg": "^0.11.6",
    "bcrypt": "^5.1.0",
    "browserify-package-json": "^1.0.1",
    "connect-mongo": "^4.6.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "express": "^4.18.2",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.3",
    "mongoose": "^6.9.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.0",
