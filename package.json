{
  "name": "webtube",
  "version": "1.0.0",
  "type": "module",
  "description": "youtube clone coding.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beom-jun-kim/beomjun-tube.git"
  },
  "scripts": {
    "start": "node build/init.js",
    "build": "babel src --out-dir build && npm run build:server && npm run build:assets",
    "build:server": "babel src -d build --ignore src/client",
    "build:assets": "webpack --mode=production",
    "dev:server": "nodemon",
    "dev:assets": "webpack --mode=development -w"
  },
